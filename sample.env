# BSCScan API Key for fetching contract ABIs and transaction history
BSCSCAN_API_KEY=

# Network Configuration
BSC_MAINNET_RPC=https://bsc-dataseed.binance.org/
BSC_TESTNET_RPC=https://data-seed-prebsc-1-s1.binance.org:8545/
BSC_MAINNET_CHAIN_ID=56
BSC_TESTNET_CHAIN_ID=97
BSC_MAINNET_EXPLORER_URL=https://bscscan.com
BSC_TESTNET_EXPLORER_URL=https://testnet.bscscan.com

# Multicall Addresses
BSC_MAINNET_MULTICALL_ADDRESS=0x41263cBA59EB80dC200F3E2544eda4ed6A90E76C
BSC_TESTNET_MULTICALL_ADDRESS=0x6e5BB1a5Ad6F68A8D7D6A5e47750eC15773d6042

# File Paths
WALLET_FILE=wallets.csv
BSC_MAINNET_TOKENS_FILE=mainnet_tokens.csv
BSC_TESTNET_TOKENS_FILE=testnet_tokens.csv

# Transaction Settings
DEFAULT_GAS_LIMIT=100000
DEFAULT_GAS_PRICE_MULTIPLIER=1.1
MAX_TRANSACTION_TIMEOUT=180
MAX_RETRIES=3

# Security Settings
ENCRYPT_PRIVATE_KEYS=false
CONVERT_CHECKSUM_ADDRESS=false
WALLET_ENCRYPTION_KEY=change_this_to_a_secure_random_string